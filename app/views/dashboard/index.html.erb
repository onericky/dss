<div>
  <div class="row">
    <div class="col s4 m4">
      <div class="card blue-grey darken-1 card-dss-w">
        <div class="card-content white-text card-dss-title-w">
          <span class="card-title">Z1 - RISK</span>
        </div>
        <div class="card-action">
          <div>
            <select class="browser-default">
              <option value="" disabled selected>Choose your option</option>
              <option value="1">Low</option>
              <option value="2">Regular</option>
              <option value="3">High</option>
            </select>
          </div>
        </div>
      </div>
    </div>
    <div class="col s4 m4">
      <div class="card blue-grey darken-1 card-dss-w">
        <div class="card-content white-text card-dss-title-w">
          <span class="card-title">Z2 - TRAFFIC</span>
        </div>
        <div class="card-action">
          <div>
            <select class="browser-default">
              <option value="" disabled selected>Choose your option</option>
              <option value="1">Low</option>
              <option value="2">Regular</option>
              <option value="3">High</option>
            </select>
          </div>
        </div>
      </div>
    </div>
    <div class="col s4 m4">
      <div class="card blue-grey darken-1 card-dss-w">
        <div class="card-content white-text card-dss-title-w">
          <span class="card-title">Z3 - WEATHER SEASON</span>
        </div>
        <div class="card-action">
          <div class="switch">
            <label>
              <span class="yellow-text">Off</span>
              <input type="checkbox">
              <span class="lever"></span>
              <span class="yellow-text">On</span>
            </label>
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col s3 m3">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">X1 - ACTIVE DELIVERY METHOD</span>
        </div>
        <div class="card-action">
          <p>
            <label>
              <input type="checkbox" class="filled-in" />
              <span class="yellow-text">Bike</span>
            </label>
          </p>
          <p>
            <label>
              <input type="checkbox" class="filled-in" />
              <span class="yellow-text">Motorbike</span>
            </label>
          </p>
          <p>
            <label>
              <input type="checkbox" class="filled-in" />
              <span class="yellow-text">Car</span>
            </label>
          </p>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col s3 m3">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">X2 - DISCOUNTS</span>
        </div>
        <div class="card-action">
          <p class="range-field">
            <input type="range" id="test5" min="0" max="100" />
          </p>
        </div>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col s3 m3">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">W1 - COST PER DELIVERY BY METHOD</span>
        </div>
        <div class="card-action">
          <div class="input-field">
            <input placeholder="Bike cost" id="bike_cost" type="number" class="validate">
            <label for="bike_cost">Bike cost</label>
          </div>
          <div class="input-field">
            <input placeholder="Motorbike cost" id="motorbike_cost" type="number" class="validate">
            <label for="motorbike_cost">Motorbike cost</label>
          </div>
          <div class="input-field">
            <input placeholder="Car cost" id="car_cost" type="number" class="validate">
            <label for="car_cost">Car cost</label>
          </div>
        </div>
      </div>
    </div>
    <div class="col s3 m3">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">W2 - OPERATING EXPENSES</span>
        </div>
        <div class="card-action">
          <div class="input-field">
            <input placeholder="Payroll" id="payroll" type="number" class="validate">
            <label for="payroll">Payroll</label>
          </div>
          <div class="input-field">
            <input placeholder="Infrastructure" id="infrastructure" type="number" class="validate">
            <label for="infrastructure">Infrastructure</label>
          </div>
          <div class="input-field">
            <input placeholder="Marketing" id="marketing" type="number" class="validate">
            <label for="marketing">Marketing</label>
          </div>
        </div>
      </div>
    </div>
    <div class="col s3 m3">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">W3 - ORDERS</span>
        </div>
        <div class="card-action">
          <table>
            <thead>
              <tr>
                <th class="center-align"><span class="white-text">Zone</span></th>
                <th class="center-align"><span class="white-text">Quick</span></th>
                <th class="center-align"><span class="white-text">Standard</span></th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td class="center-align"><span class="white-text">A</span></td>
                <td><input id="quick-A" type="number" class="validate"></td>
                <td><input id="standard-A" type="number" class="validate"></td>
              </tr>
              <tr>
                <td class="center-align"><span class="white-text">B</span></td>
                <td><input id="quick-B" type="number" class="validate"></td>
                <td><input id="standard-B" type="number" class="validate"></td>
              </tr>
              <tr>
                <td class="center-align"><span class="white-text">C</span></td>
                <td><input id="quick-C" type="number" class="validate"></td>
                <td><input id="standard-C" type="number" class="validate"></td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
    <div class="col s3 m3">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">W4 - QUANTITY OF VEHICLES</span>
        </div>
        <div class="card-action">
          <<div class="input-field">
          <input placeholder="Bikes" id="bike_qty" type="number" class="validate">
          <label for="bike_qty">Bikes</label>
        </div>
          <div class="input-field">
            <input placeholder="Motorbikes" id="motorbike_qty" type="number" class="validate">
            <label for="motorbike_qty">Motorbikes</label>
          </div>
          <div class="input-field">
            <input placeholder="Cars" id="car_qty" type="number" class="validate">
            <label for="car_qty">Cars</label>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>