<thead>
  <tr>
    <th></th>
    <% column_number = 0 %>
    <% if data.sample.is_a?(Array)%>
      <% columns = data.sample.length - 1 %>
    <% else %>
      <% columns = 1 %>
    <% end %>
    <% columns.times do %>
      <th>Column: <%=column_number%></th>
      <% column_number += 1 %>
    <% end %>
  </tr>
</thead>
